{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "noImplicitThis": false,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {}
  },
  "include": ["src/**/*.ts", "run-tests.ts"]
  ,
  "exclude": ["**/*.spec.ts"]
}
